{
  "manifest_version": 2,
  "name": "AWS Form Dump",
  "description": "Dump all AWS form field (including AWS custom select field) as JSON (`{\"name\": \"value\"}`) and copy.",
  "version": "0.0.1",
  "homepage_url": "https://github.com/huguesbr/aws-dump-form",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Dump AWS Form"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "js": ["jquery.min.js"]
    }
  ]
}
